# Archipel runtime defaults
ARCHIPEL_PORT=7777
ARCHIPEL_HELLO_INTERVAL=30
ARCHIPEL_STATE_DIR=.archipel

# Optional: 64 hex chars (32 bytes). If empty, a zero-key placeholder is used.
ARCHIPEL_CONTROL_HMAC_KEY=

# Optional Gemini integration
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# Web UI defaults
ARCHIPEL_WEB_PORT=8080
